
#include "pid.h"
void PidInit(PID_typedef* RegolatorePid){
	RegolatorePid->prev_error=0;    //Variabile errore iterazione precedente
	RegolatorePid->integrale=0;     //Variabile azione integrale
	RegolatorePid->error=0;
	RegolatorePid->derivata=0;
	RegolatorePid->output=0;
	RegolatorePid->abserror=0;
	RegolatorePid->input=0;
}

