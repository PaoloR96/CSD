#include"pid.h"
PidTypedef pid;
float max_output = 100.0f;      // Massimo valore di uscita
float min_output = 0.0f;        // Minimo valore di uscita
void pid_init(void)
{
    pid->integral_temp = 0.0f;
    pid->integral_hum = 0.0f;
    pid->last_error_temp = 0.0f;
    pid->last_error_hum = 0.0f;
}
